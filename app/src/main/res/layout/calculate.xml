<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#bde3fc"
    tools:context=".MainActivity">

    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="512dp"
        android:layout_height="414dp"
        android:background="#9dceef"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.497"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.666">

    </androidx.constraintlayout.widget.ConstraintLayout>

    <TextView
        android:id="@+id/textView"
        android:layout_width="43dp"
        android:layout_height="57dp"
        android:layout_weight="1"
        android:text="Name:"
        app:layout_constraintBottom_toBottomOf="@+id/calcName"
        app:layout_constraintEnd_toStartOf="@+id/calcName"
        app:layout_constraintTop_toTopOf="@+id/calcName"
        app:layout_constraintVertical_bias="0.0" />

    <TextView
        android:id="@+id/textView4"
        android:layout_width="81dp"
        android:layout_height="47dp"
        android:layout_weight="1"
        android:text="Total Hours:"
        app:layout_constraintBottom_toBottomOf="@+id/calcHours"
        app:layout_constraintEnd_toStartOf="@+id/calcHours"
        app:layout_constraintTop_toTopOf="@+id/calcHours"
        app:layout_constraintVertical_bias="0.0" />

    <TextView
        android:id="@+id/textView5"
        android:layout_width="81dp"
        android:layout_height="47dp"
        android:layout_weight="1"
        android:text="Total Wage Received"
        app:layout_constraintBottom_toBottomOf="@+id/totalWage"
        app:layout_constraintEnd_toStartOf="@+id/totalWage"
        app:layout_constraintTop_toTopOf="@+id/totalWage"
        app:layout_constraintVertical_bias="0.526" />

    <TextView
        android:id="@+id/textView6"
        android:layout_width="101dp"
        android:layout_height="50dp"
        android:layout_weight="1"
        android:text="Overtime Hours"
        app:layout_constraintBottom_toBottomOf="@+id/otHours"
        app:layout_constraintEnd_toStartOf="@+id/otHours"
        app:layout_constraintTop_toBottomOf="@+id/totalWage" />

    <TextView
        android:id="@+id/textView7"
        android:layout_width="97dp"
        android:layout_height="41dp"
        android:layout_weight="1"
        android:text="Overtime Wage"
        app:layout_constraintBottom_toBottomOf="@+id/otWage"
        app:layout_constraintEnd_toStartOf="@+id/otWage"
        app:layout_constraintTop_toBottomOf="@+id/otHours" />

    <TextView
        android:id="@+id/calcName"
        android:layout_width="152dp"
        android:layout_height="66dp"
        android:layout_weight="1"
        android:text="TextView"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.499"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.084" />

    <TextView
        android:id="@+id/calcHours"
        android:layout_width="152dp"
        android:layout_height="66dp"
        android:layout_weight="1"
        android:text="TextView"
        app:layout_constraintEnd_toEndOf="@+id/calcName"
        app:layout_constraintStart_toStartOf="@+id/calcName"
        app:layout_constraintTop_toBottomOf="@+id/calcName" />

    <TextView
        android:id="@+id/totalWage"
        android:layout_width="152dp"
        android:layout_height="66dp"
        android:layout_weight="1"
        android:text="TextView"
        app:layout_constraintEnd_toEndOf="@+id/calcHours"
        app:layout_constraintStart_toStartOf="@+id/calcHours"
        app:layout_constraintTop_toBottomOf="@+id/calcHours" />

    <TextView
        android:id="@+id/otHours"
        android:layout_width="152dp"
        android:layout_height="66dp"
        android:layout_weight="1"
        android:text="TextView"
        app:layout_constraintEnd_toEndOf="@+id/totalWage"
        app:layout_constraintStart_toEndOf="@+id/textView5"
        app:layout_constraintTop_toBottomOf="@+id/totalWage" />

    <TextView
        android:id="@+id/otWage"
        android:layout_width="152dp"
        android:layout_height="66dp"
        android:layout_weight="1"
        android:text="TextView"
        app:layout_constraintEnd_toEndOf="@+id/otHours"
        app:layout_constraintStart_toStartOf="@+id/otHours"
        app:layout_constraintTop_toBottomOf="@+id/otHours" />

    <Button
        android:id="@+id/goBackBTN"
        android:layout_width="114dp"
        android:layout_height="48dp"
        android:background="@drawable/custombutton"
        android:text="Go Back"
        app:backgroundTint="@null"
        app:layout_constraintEnd_toEndOf="@+id/otWage"
        app:layout_constraintStart_toEndOf="@+id/textView7"
        app:layout_constraintTop_toBottomOf="@+id/otWage" />

</androidx.constraintlayout.widget.ConstraintLayout>